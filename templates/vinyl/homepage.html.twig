{% extends 'base.html.twig' %}

{% block title %}Create a new Record | {{ parent() }}{% endblock %}

{% block body %}
<h1>{{ title }}</h1>

<div>
Tracks:
    <ul>
        {% for track in tracks %}
            <li>song: {{ track.song }} - artist: {{ track.artist|upper }}</li>
        {% endfor %}
    </ul>
</div>
{% endblock %}